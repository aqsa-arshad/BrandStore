          <xsd:restriction base="xsd:string">
            <xsd:pattern value="(true)|(false)" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:attribute>
      <xsd:attribute name="Type" type="SystemDataType" use="required" />
      <xsd:attribute name="Default" type="xsd:string" use="required" />
    </xsd:complexType>
  </xsd:element>
