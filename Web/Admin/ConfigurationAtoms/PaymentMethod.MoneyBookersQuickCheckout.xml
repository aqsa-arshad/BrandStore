  <Title>Configure Skrill (Moneybookers) Quick Checkout</Title>


  <AppConfig Required="false" Advanced="false" FriendlyName="User Login">
    <Name>Moneybookers.UserLogin</Name>
  </AppConfig>

  <AppConfig Required="false" Advanced="false" FriendlyName="User Password">
    <Name>Moneybookers.UserPassword</Name>
  </AppConfig>

  <AppConfig Required="false" Advanced="false" FriendlyName="Sender ID">
    <Name>Moneybookers.SenderID</Name>
  </AppConfig>

  <AppConfig Required="false" Advanced="false" FriendlyName="Channel ID">
    <Name>Moneybookers.ChannelID</Name>
  </AppConfig>

  <AppConfig Required="false" Advanced="false" FriendlyName="Quick Checkout Payment Icon">
    <Name>Moneybookers.QuickCheckout.PaymentIcon</Name>
  </AppConfig>

  <AppConfig Required="false" Advanced="false" FriendlyName="Hide Login for Quick Checkout">
    <Name>Moneybookers.QuickCheckout.HideLogin</Name>
  </AppConfig>

</ConfigurationAtom>